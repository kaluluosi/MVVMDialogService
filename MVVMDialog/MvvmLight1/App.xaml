<Application x:Class="MvvmLight1.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:vm="clr-namespace:MvvmLight1.ViewModel"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:dlg="clr-namespace:GalaSoft.MvvmLight.Dialog;assembly=GalaSoft.MvvmLight.Dialog"
             xmlns:v="clr-namespace:MvvmLight1.View"
             xmlns:pane="clr-namespace:MvvmLight1.View.PaneTemplate"
             StartupUri="MainWindow.xaml"
             mc:Ignorable="d"
             >
    
    <Application.Resources>
        <!--Global View Model Locator-->
        <vm:ViewModelLocator x:Key="Locator"
                             d:IsDataSource="True" />
        <pane:PanelTemplateManager x:Key="PaneMgr">
            
        </pane:PanelTemplateManager>
        
    </Application.Resources>
    
</Application>
